@article{caramel-juvino_caries_2023,
	title = {Caries Preventive Action of Nd:{YAG} and Fluoride in Three Different {pH} Conditions: {FTIR} Spectroscopy and {SEM} Evaluation},
	volume = {10},
	rights = {https://creativecommons.org/licenses/by/4.0/},
	issn = {2304-6732},
	url = {https://www.mdpi.com/2304-6732/10/9/985},
	doi = {10.3390/photonics10090985},
	shorttitle = {Caries Preventive Action of Nd},
	abstract = {This in vitro study aimed to evaluate the preventive action of topical fluoride application combined with laser irradiation under different {pH} conditions using Fourier Transform Infrared Spectroscopy ({FTIR}) and Scanning Electron Microscopy ({SEM}). A total of 180 samples of human dental enamel were prepared and divided into groups: Negative Control, Fluoride ({FFA} 12.300 µF−/g), Laser (Nd:{YAG} 84.9 J/cm2), and Laser + Fluoride (Nd:{YAG} 84.9 J/cm2 + {FFA} 12.300 µF−/g). The {pH} cycling was performed at three different {pH} conditions: {pH} 5 (below the critical {pH} for hydroxyapatite), {pH} 4.5 (below the critical {pH} in the presence of fluorapatite), and {pH} 4 (investigating acid resistance of hydroxyapatite and fluorapatite forms with laser irradiation). In the {FTIR} analysis, the Laser + Fluoride group demonstrated statistically significant differences compared to the Negative Control group and Fluoride group at {pH} 4.5 and {pH} 4 when evaluating the phosphate bands. Similar results were observed in the {SEM} analysis, where the Laser + Fluoride group exhibited lower demineralization compared to the other treatments at {pH} 4.5 and {pH} 4. In conclusion, the Laser + Fluoride group demonstrated a significant reduction in demineralization even at {pH} levels below the critical threshold for fluorapatite, highlighting its superior acid resistance compared to fluoride alone.},
	pages = {985},
	number = {9},
	journaltitle = {Photonics},
	shortjournal = {Photonics},
	author = {Caramel-Juvino, Amanda and Zanini, Nathalia A. and Avelino, Sabrina Gardiano and Fontes-Oliveira, Yasmin Reis and Germano, Gleice Conceição Mendonça and Eduardo, Carlos De Paula and Zezell, Denise Maria},
	urldate = {2025-03-21},
	date = {2023-08-29},
	langid = {english},
	file = {Texto completo:/home/mario/Zotero/storage/4NYUUF38/Caramel-Juvino et al. - 2023 - Caries Preventive Action of NdYAG and Fluoride in Three Different pH Conditions FTIR Spectroscopy.pdf:application/pdf},
}

