	volume = {33},
	issn = {01039393},
	url = {https://brazilianperiodontology.com/artigo/966/Periodontology-2023-v33n4/13746},
	doi = {10.14436/0103-9393.33.4.034-042.oar},
	shorttitle = {Fibroma ossificante periférico maxilar gigante},
	abstract = {{INTRODUÇÃO}: O fibroma ossificante periférico é uma doença gengival possivelmente originada nas células do ligamento periodontal. {OBJETIVO}: O objetivo do presente artigo é relatar o caso clínico de uma paciente do sexo feminino, 30 anos de idade, em tratamento ortodôntico, que apresentou nódulo exofítico, séssil, duro, bem delimitado, medindo 2,5 cm, sem ulceração. {CASO} {CLÍNICO}: A lesão, localizada entre o canino e o primeiro pré-molar superiores direitos, evoluiu ao longo de seis meses. Após exames pré-operatórios e confirmação da hipótese diagnóstica por meio do exame radiográfico, foi realizada biópsia excisional. A análise histológica da peça confirmou o diagnóstico de fibroma ossificante periférico. {CONCLUSÃO}: O presente relato de caso clínico de fibroma ossificante periférico reforça a importância da discussão sobre essa doença no diagnóstico diferencial de lesões gengivais.},
	pages = {34--42},
	number = {4},
	journaltitle = {Brazilian Journal of Periodontology},
	shortjournal = {Braz J Periodontol.},
	author = {Barreto, Anna Laura Almeida and Monteiro, Gabriela Reganin and Peralta, Felipe Da Silva and Kitakawa, Dárcio and Carvalho, Luis Felipe Das Chagas E Silva De},
	urldate = {2025-03-24},
	date = {2023-12-30},
}

@article{melo_de_lima_morais_low-level_2024,
	title = {Low-level laser therapy as an adjuvant in the treatment of erythema multiforme of the oral mucosa: a case report},
	volume = {31},
	rights = {http://creativecommons.org/licenses/by-nc/4.0},
	issn = {1884-7269, 0898-5901},
	url = {https://www.lasertherapyjournal.com/ltj/article/view/386},
	doi = {10.4081/ltj.2024.386},
	shorttitle = {Low-level laser therapy as an adjuvant in the treatment of erythema multiforme of the oral mucosa},
	abstract = {Erythema multiforme is an autoimmune condition that can affect the skin and mucosa. Oral lesions initially present with edema and progress to superficial erosions with pseudomembrane formation. The most recommended treatment is the use of corticosteroids; however, low-level laser therapy can be effective in the treatment of erythema multiforme. We report a case of erythema multiforme in the oral mucosa treated with low-level laser therapy. A 73-year-old woman using alendronate for osteoporosis, losartan, and puran T4 with extensive ulcers on the upper and lower lips. The clinical diagnosis was erythema multiforme. The proposed treatment was 0.05\% clobetasol propionate in gel, 3 times a day, and seven sessions of low-level laser therapy on alternate days. Low-level laser therapy significantly improved the erythema multiforme of the oral mucosa, offering the patient a non-invasive approach with no side effects.},
	number = {1},
	journaltitle = {Laser Therapy},
	shortjournal = {Laser Ther J},
	author = {Melo De Lima Morais, Thayná and Silva, Sara Maria Santos Dias Da and Da Silva Peralta, Felipe and Kitakawa, Dárcio and Saito Nogueira, Marcelo and Das Chagas E Silva De Carvalho, Luis Felipe},
	urldate = {2025-03-24},
	date = {2024-03-05},
	file = {Texto completo:/home/mario/Zotero/storage/VCDPSZVB/Melo De Lima Morais et al. - 2024 - Low-level laser therapy as an adjuvant in the treatment of erythema multiforme of the oral mucosa a.pdf:application/pdf},
