@article{ciabattari_distribuicao_2022,
	title = {Distribuição geográfica e situação de atendimento dos equipamentos {PET}-{CT} {NO} {BRASIL}, considerados os fatores distância e população: estudo e proposta a partir dos dados do {DATASUS}},
	url = {https://dspace.mackenzie.br/handle/10899/30950},
	shorttitle = {Distribuição geográfica e situação de atendimento dos equipamentos {PET}-{CT} {NO} {BRASIL}, considerados os fatores distância e população},
	abstract = {As distâncias aos equipamentos de diagnóstico {PET}-{CT} precisam ser reduzidas para que pacientes tenham um rápido diagnóstico de doenças, e como consequência, maior chance de sobrevivência através da detecção e início de tratamento forma precoce. Logo, este estudo vai abordar a análise de como estão distribuídos os equipamentos {PET}-{CT} no território brasileiro, e o que deve ser feito para melhorar o acesso da população a este. A plotagem de dados em mapas para visualização e análise de resultados demonstraram que Brasil possui uma quantidade insuficiente de {PET}-{CTs} para atender de forma adequada às demandas da população em relação aos exames específicos de diagnostico, proporcionados somente por este equipamento. Então, uma proposta de localização de novos equipamentos foi realizada através do solver Location-allocation Problem com a utilização dos métodos Minimize Weighted Impedance (P-Median), Maximize Coverage e Maximize Attendance. O algoritmo em linguagem Python foi executado dentro de um notebook no ambiente do software {ArcGIS} Pro Desktop, e como resultado, dentro dos parâmetros introduzidos, indicou a necessidade de 512 equipamentos adicionais aos 119 já existentes no pais. E, ainda assim, devido a heterogeneidade do território brasileiro, 15.403.911 habitantes ficaram sem acesso a um {PET}-{CT} dentro das restrições determinadas no algoritmo},
	author = {Ciabattari, Felipe Marlieri Cabral and Souza, Thiago Rodrigues de},
	urldate = {2025-04-11},
	date = {2022-06},
	langid = {brazil},
	note = {Publisher: Universidade Presbiteriana Mackenzie},
	file = {Full Text PDF:/home/mario/Zotero/storage/U2PMA74V/Ciabattari e Souza - 2022 - Distribuição geográfica e situação de atendimento dos equipamentos PET-CT NO BRASIL, considerados os.pdf:application/pdf},
}

@article{ribeiro_clinical_2024,
	title = {A Clinical Appearance of Traumatic Fibroma with a Distinct and Rare Histological Diagnosis –insights of Pseudo Perineurioma – Case Report},
	volume = {18},
	url = {https://opendentistryjournal.com/abs/10.2174/0118742106284558240424080759},
	doi = {10.2174/0118742106284558240424080759},
	abstract = {Oral pseudo perineurioma is a reactive lesion of the peripheral nerve sheath rarely described in the literature, occurring more frequently in the tongue and lower lips.The objective of this case report is to describe pseudoperineurioma with a literary basis, to encourage learning and assist in decision-making in certain situations.We present here a rare case of oral pseudoperineurioma in tongue that occurred in a 19-year-old man and a brief review of the literature. A 19-year-old man presented with a nodular lesion on the dorsum of the tongue without painful symptoms. Excisional biopsy was performed. The pathological examination revealed a proliferation of structures like onion bulbs. Immunohistochemical analysis was positive for {GLUT}-1.The histological diagnosis was oral pseudoperineurioma. Oral pseudoperineurioma is a rare clinical lesion and can be confused with other reactive and neoplastic conditions of the oral cavity.},
	journaltitle = {The Open Dentistry Journal},
	author = {Ribeiro, Carnevalli Ana Clara and Santos, Dias da Silva Sara Maria and Sandy, da Silva Leite Larissa and Tavares, de Moura Vitória and Dárcio, Kitakawa and Felipe, da Silva Peralta and Melo, de Lima Morais Thayná and and, de Carvalho Luis Felipe das Chagas e Silva},
	date = {2024-05-07},
